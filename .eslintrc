{
"plugins": [
"react"
],
"extends": ["eslint:recommended", "plugin:react/recommended"],
"env":{
    "node": true,
    "mocha": true,
    "meteor": true
    },
"parserOptions":{
    "ecmaVersion": 6,
    "sourceType":"module"
    },
    "rules":{
        "no-console": "warn",
        "no-unused-vars": "warn"
    }
}
